{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [true],
        "no-console": [false],
        "quotemark": [true, "single"],
        "prefer-for-of": true,
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "no-empty": true,
        "no-trailing-whitespace": [true, "ignore-blank-lines"],
        "no-consecutive-blank-lines": false,
        "ordered-imports": false,
        "trailing-comma": true,
        "member-access": false,
        "whitespace": [true, "check-separator", "check-branch"],
        "variable-name": [true, "allow-leading-underscore", "allow-pascal-case"]
    },
    "rulesDirectory": []
}
